<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.web.WebView?>
<fx:root type="javafx.scene.control.Tab" xmlns:fx="http://javafx.com/fxml">
    <VBox spacing="15" fx:id="vbox1">
        <padding><Insets top="15" right="15" bottom="15" left="15"></Insets></padding>

        <HBox spacing="30">
            <ScrollPane prefWidth="200" prefHeight="580">
                <Accordion prefWidth="200">
                    <panes>
                        <TitledPane >
                            <text>新建流程</text>
                            <content>
                                <ListView fx:id="newProcess">
                                </ListView>
                            </content>
                        </TitledPane>
                        <TitledPane>
                            <text>任务处理</text>
                            <content>
                                <TreeView fx:id="taskHandle">
                                </TreeView>
                            </content>
                        </TitledPane>
                    </panes>
                </Accordion>
            </ScrollPane>
            <WebView prefWidth="400" prefHeight="580">

            </WebView>

        </HBox>

    </VBox>

</fx:root>