<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<fx:root type="javafx.scene.control.Tab" xmlns:fx="http://javafx.com/fxml">
    <HBox spacing="15" fx:id="hbox1">
        <padding><Insets top="15" right="15" bottom="15" left="15"></Insets></padding>
        <Label fx:id="infolabel" minWidth="200">信息\n</Label>
        <VBox spacing="25">
            <Button fx:id="openEPML" onAction="#onOpenEPMLAction">OpenEPML</Button>
            <Button fx:id="parse" onAction="#onParseAction">Parse</Button>
        </VBox>

    </HBox>

</fx:root>